{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["closure","jsdoc"]
    },
    "source": {
	"include": ["tart","README.md"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "third_party/jaguarjs-jsdoc",  
        "encoding": "utf8",              
        "destination": "docs/",          
        "recurse": true
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
    	"applicationName": "TartJS",
    	"disqus": "",
    	"googleAnalytics": "",
    	"openGraph": {
      	    "title": "",
            "type": "",
            "image": "",
            "site_name": "",
            "url": ""
    	},
    	"meta": {
            "title": "tartJS",
            "description": "",
            "keyword": ""
    	},
        "linenums": true
    },
    "markdown": {
	"tags": []
    }
}
